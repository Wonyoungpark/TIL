package ch05.sec12;

public class WeekExample {

	public static void main(String[] args) {
		//열거 타입 변수를 선언
		Week today = null; //Week는 참조타입
		
		//열거값 대입
		today = Week.THURSDAY;
		
		//열거값 비교
		if(today==Week.FRIDAY) {
			System.out.println("오늘은 불금이 아닙니다.");
		} else System.out.println("오늘은 금요일");
	}

}
